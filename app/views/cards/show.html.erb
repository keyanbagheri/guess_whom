<% if @card %>
  <div class="card_focus">
        <div>
          <p><img src="<%= @card.instagram_picture_url %>"/></p>
          <h2><%= @card.username %></h2>
            <h4><a href="/cards/<%=@card.id%>/update">edit</a> | <a href="/cards/<%= @card.id %>/delete">delete</a></h4>
          </p>
        </div>
  </div>
<% else %>
  <div class="card">
    No card found with that ID!
  </div>
<% end %>
